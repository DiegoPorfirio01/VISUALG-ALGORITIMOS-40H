Algoritmo "SOMADOR NUMERICO"



Var

   maior, cont, N, S:INTEIRO


Inicio
   cont <- 1
   S <- 0
   enquanto ( cont <=5) faca
      escreva ( " digite o valor ", cont , "o. valor : " )
      leia (N)
      se (N > maior) ENTAO
         maior <- N
      fimse
      S <- S + N
      cont <- cont + 1




   fimenquanto
   ESCREVAL (S)


Fimalgoritmo