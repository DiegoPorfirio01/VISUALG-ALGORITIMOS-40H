Algoritmo "NUMEROsomaEmaior"



Var
  contador, valor, salto: inteiro

Inicio
   contador <- 1
   escreval ("quer contr até quanto ? ")
   leia (valor)
   escreval (" quantos saltos ? ")
   leia (salto)
   enquanto ( contador <= valor ) faca
      escreval(contador)
      contador <- contador + salto
   fimenquanto
   escreval (" TERMINEI DE CONTAR " )



Fimalgoritmo