Algoritmo "NUMEROsomaEmaior"



Var
  contador, valor: inteiro

Inicio
   contador <- 1
   escreval ("quer contr até quanto ? ")
   leia (valor)
   enquanto ( contador <= valor ) faca
      escreval(contador)
      contador <- contador + 1
   fimenquanto
   escreval (" TERMINEI DE CONTAR " )



Fimalgoritmo